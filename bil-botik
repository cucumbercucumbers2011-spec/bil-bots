from telegram import InlineKeyboardButton, InlineKeyboardMarkup, Update
from telegram.ext import Application, CommandHandler, CallbackQueryHandler, ContextTypes


TOKEN = "7884121771:AAEtkFHESFkA7duB4zTK99txTbKPUPO4g8Q"

#  –ë–∞—Å—Ç—ã 
async def start(update: Update, context: ContextTypes.DEFAULT_TYPE):
    keyboard = [
        [InlineKeyboardButton("üìã –ï—Ä–µ–∂–µ–ª–µ—Ä", callback_data="rules")],
        [InlineKeyboardButton("üîó Bill-–≥–∞ “õ–∞—Ç—ã—Å—Ç—ã –°—Å—ã–ª–∫–∞–ª–∞—Ä", callback_data="links")],
        [InlineKeyboardButton("üìû –ù–æ–º–µ—Ä–ª–µ—Ä", callback_data="contacts")]
    ]
    reply_markup = InlineKeyboardMarkup(keyboard)
    await update.message.reply_text("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å! –í—ã–±–µ—Ä–∏ –ø—É–Ω–∫—Ç –º–µ–Ω—é:", reply_markup=reply_markup)

# –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–Ω–æ–ø–æ–∫
async def button(update: Update, context: ContextTypes.DEFAULT_TYPE):
    query = update.callback_query
    await query.answer()

    if query.data == "rules":
        keyboard = [
            [InlineKeyboardButton("üçΩ –ê—Å—Ö–∞–Ω–∞", callback_data="rule_stol")],
            [InlineKeyboardButton("üè† –ñ–∞—Ç–∞“õ—Ö–∞–Ω–∞", callback_data="rule_obsh")],
            [InlineKeyboardButton("üöª –î”ô—Ä–µ—Ç—Ö–∞–Ω–∞", callback_data="rule_tualet")],
            [InlineKeyboardButton("üìö –°—ã–Ω—ã–ø", callback_data="rule_class")],
            [InlineKeyboardButton("‚úÖ –¢”ô—Ä—Ç—ñ–ø", callback_data="rule_behave")],
            [InlineKeyboardButton("üôè “Æ–ª–∫–µ–Ω–¥–µ—Ä–≥–µ “õ“±—Ä–º–µ—Ç", callback_data="rule_respect")],
            [InlineKeyboardButton("‚¨ÖÔ∏è –ê—Ä—Ç“õ–∞", callback_data="back_to_menu")]
        ]
        reply_markup = InlineKeyboardMarkup(keyboard)
        await query.edit_message_text("–í—ã–±–µ—Ä–∏ –∫–∞—Ç–µ–≥–æ—Ä–∏—é –ø—Ä–∞–≤–∏–ª:", reply_markup=reply_markup)

    elif query.data == "links":
        await query.edit_message_text("üîó –ü–∞–π–¥–∞–ª—ã —Å—Å—ã–ª–∫–∞–ª–∞—Ä:\n- Bilim Class:https://www.bilimclass.kz\n- Bilorg:https://bil.org.kz ")
    elif query.data == "contacts":
